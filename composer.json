{
    "name": "tristanward/laravel-workable",
    "description": "A Laravel wrapper for the Workable API. Easily access and cache Workable vacancies from your Workable account using the official Workable API.",
    "keywords": ["laravel","workable","wrapper"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tristan Ward",
            "email": "tristanward1989@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^6.3@dev"
    },
    "autoload": {
        "psr-4": {
            "Tristanward\\LaravelWorkable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tristanward\\LaravelWorkable\\Providers\\LaravelWorkableServiceProvider"
            ],
            "aliases": {
                "LaravelWorkable": "Tristanward\\LaravelWorkable\\Facades\\LaravelWorkable"
            }
        }
    }
}